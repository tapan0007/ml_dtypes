#! /usr/bin/env  bash

set -x

yum_packages="python36 gcc64-c++ gcc48-c++ libzip-devel zlib-static createrepo aws-cli unzip python36-devel"
pip_packages="numpy pillow keras graphviz tensorflow==1.5 filelock boto3 psutil scikit-image"


yum -y install $yum_packages
pip-3.6 install --user $pip_packages
